<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="v18072023-1-drop-value-token-column" author="azakhvalinskiy">
        <preConditions onFail="MARK_RAN">
            <and>
                <tableExists tableName="auth_tokens"/>
                <columnExists tableName="auth_tokens" columnName="value"/>
            </and>
        </preConditions>
        <dropColumn tableName="auth_tokens" columnName="value"/>
    </changeSet>
</databaseChangeLog>